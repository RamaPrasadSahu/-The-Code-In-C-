// First Program Starts
#include <stdio.h>
#include <string.h>

struct structure {
    int rollno;
    char name[30];
    float mark;
} Rm1, Rm2;

int main() {
 
    strcpy(Rm1.name, "Rama");
    Rm1.rollno = 20;

  
    printf("%s\n", Rm1.name);
    printf("%d\n", Rm1.rollno);

    return 0;
}

// End of First Program 
